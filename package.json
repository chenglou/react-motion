{
  "name": "@allthings/react-motion",
  "version": "1.0.0",
  "description": "A spring that solves your animation problems.",
  "main": "lib/react-motion.js",
  "module": "lib/react-motion.esm.js",
  "types": "types",
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-modules-commonjs": "7.15.4",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime": "7.15.4",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "@rollup/plugin-replace": "3.0.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "codemirror": "5.63.1",
    "cross-env": "7.0.3",
    "css-loader": "6.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "flow-bin": "0.160.2",
    "flow-copy-source": "2.0.9",
    "husky": "7.0.2",
    "inject-loader": "4.0.1",
    "isparta-loader": "2.0.0",
    "jasmine-core": "3.9.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-diff-reporter": "2.0.1",
    "karma-webpack": "5.0.0",
    "lint-staged": "11.2.0",
    "lodash.range": "3.2.0",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-uglify": "6.0.4",
    "style-loader": "3.3.0",
    "webpack": "5.57.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.3.1"
  },
  "scripts": {
    "start": "node server.js",
    "prebuild:dist": "rm -rf build",
    "build:dist": "rollup -c",
    "prebuild:lib": "rm -rf lib",
    "build:lib": "babel src --out-dir lib",
    "build:flow": "flow-copy-source -v src lib",
    "build": "yarn build:dist && yarn build:lib && yarn build:flow",
    "build-demos": "webpack",
    "lint": "eslint --ext .js,.jsx .",
    "flow_check": "flow check",
    "prepublishOnly": "yarn build",
    "test": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run",
    "test:travis": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run",
    "test:dev": "cross-env NODE_ENV=test karma start ./karma.conf.js --no-single-run --auto-watch",
    "test:cov": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run --reporters coverage",
    "gh-pages": "git fetch origin && git checkout gh-pages && git reset --hard origin/gh-pages && git rebase origin/master --force-rebase && npm run build-demos && git add . && git commit --amend --no-edit && git push origin gh-pages --force && git checkout master",
    "postversion": "git push --tags origin HEAD",
    "precommit": "lint-staged",
    "security-check": "yarn audit --groups dependencies --level high || test $? -lt 7",
    "up": "yarn upgrade-interactive --latest --exact"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17",
    "react-dom": "^16.8 || ^17"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allthings/react-motion.git"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "transitiongroup",
    "spring",
    "tween",
    "motion",
    "animation",
    "transition",
    "ui"
  ],
  "author": "chenglou",
  "license": "MIT",
  "dependencies": {
    "core-js": "3.18.2",
    "performance-now": "^2.1.0",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1"
  }
}
